	import java.util.Scanner;
	public class Date{

		int day;
		int month;
		int year;
		Date()
		{
			day=1;
			month=1;
			year=1990;
		}
		public int getDay()
		{
			return day;
		}
		public void setDay(int day)
		{
			this.day=day;
			
		}
		public int getMonth()
		{
			return month;
		}
		public void setMonth(int month)
		{
			this.month=month;
		}
		public int getYear()
		{
			return year;
		}
		public void setYear(int year)
		{
			this.year=year;
		}
		public void displayDate()
		{
			System.out.println("entered date is:"+getDay()+"/"+getMonth()+"/"+getYear());
		}
	public static void main(String args[])
	{
		Date d=new Date();
		Scanner sc=new Scanner(System.in);
		System.out.println("\nenter day between 1-31");
		int di=sc.nextInt();
		d.setDay(di);
		System.out.println("Enter the month between 1-12");
		int m=sc.nextInt();
	    d.setMonth(m);
	    System.out.println("enter the year:");
	    int y=sc.nextInt();
	    d.setYear(y);
	    d.displayDate();
	    }
	}


